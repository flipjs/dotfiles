abbr gst="git status"
abbr ls=exa
abbr pingg=prettyping
abbr son='sonos --zone=RINCON_48A6B88CEB8601400:3114580280'
abbr sonv='sonos --zone=RINCON_48A6B88CEB8601400:3114580280 volume'
