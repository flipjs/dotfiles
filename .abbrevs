# zsh-abbr

abbr compgif='gifsicle --batch -V -O2'
abbr compjpg='jpegoptim -m80'
abbr comppng='optipng -o7'
abbr ga="git add"
abbr gc="git commit -v"
abbr gcd="git checkout develop"
abbr gcm="git checkout master"
abbr gcmod='git diff-tree --no-commit-id --name-only -r'
abbr gco="git checkout"
abbr gd='git diff'
abbr gdfix='git diff --name-only | uniq | xargs nvim'
abbr gdn='git --no-pager diff'
