# zsh-abbr

abbr gc="git commit -v"
abbr gcd="git checkout develop"
abbr gcm="git checkout master"
abbr gco="git checkout"
abbr gmodvim='nvim $(git ls-files --modified)'
abbr gnewvim='nvim $(git ls-files -o  --exclude-standard)'
abbr gp="git push"
abbr gst="git status"
abbr ls=exa
abbr pingg=prettyping
abbr son='sonos --zone=RINCON_48A6B88CEB8601400:3114580280'
abbr sonv='sonos --zone=RINCON_48A6B88CEB8601400:3114580280 volume'
abbr vabbr='vim $HOME/.dotfiles/.abbrevs'
abbr valias='vim $HOME/.dotfiles/.aliases'
abbr comppng='optipng -o7'
abbr compjpg='jpegoptim -m80'
abbr compgif='gifsicle --batch -V -O2'

