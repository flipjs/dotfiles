{
  "preset": "google",
  "esnext": true,
  "validateQuoteMarks": true,
  "esprima": "babel-jscs", // global
  "esprima": "./node_modules/babel-jscs", // local
  "verbose": true, // prints out rule names
  "fileExtensions": [".js", ".jsx"],
  "maximumLineLength": 120,
  "excludeFiles": ["node_modules/**"]
}
